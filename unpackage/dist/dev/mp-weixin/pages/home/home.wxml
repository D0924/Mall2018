<view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{swiperList}}" wx:for-item="swiperItem" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['handlerSwiperClick',['$0'],[[['swiperList','',index,'goods_id']]]]]]]}}" bindtap="__e"><view class="swiper-item"><image src="{{swiperItem.image_src}}"></image></view></swiper-item></block></swiper><view class="nav-list"><block wx:for="{{categoryList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['handlerCategoryClick',['$0'],[[['categoryList','',index]]]]]]]}}" bindtap="__e"><image class="img" src="{{item.image_src}}"></image></view></block></view><view class="floor-list"><block wx:for="{{floorList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="floor-item"><image class="floor-title" src="{{item.floor_title.image_src}}"></image><view class="floor-wrap"><view class="left"><navigator url="{{item.product_list[0].url}}"><image style="{{'width:'+(item.product_list[0].image_width+'rpx')+';'}}" src="{{item.product_list[0].image_src}}"></image></navigator></view><view class="right"><block wx:for="{{item.product_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index!==0}}"><view><navigator url="{{item.url}}"><image style="{{'width:'+(item.image_width+'rpx')+';'}}" src="{{item.image_src}}" mode="widthFix"></image></navigator></view></block></block></view></view></view></block></view></view>